{
  "name": "devrsi0n-blog",
  "private": true,
  "description": "Devrsi0n's personal blog",
  "version": "1.0.0",
  "author": "devrsi0n <devrsi0n@gmail.com>",
  "homepage": "https://devrsi0n.com",
  "keywords": [
    "devrsi0n",
    "gatsby",
    "zeit",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "predev": "gatsby clean",
    "dev": "gatsby develop -o",
    "start": "concurrently \"yarn run dev\" \"delay 60 && yarn run codegen\"",
    "clean": "gatsby clean",
    "build": "yarn run clean && gatsby build",
    "serve": "gatsby serve",
    "analyze": "cross-env ANALYZE=true gatsby build",
    "lint": "yarn run eslint",
    "update-last-updated-date": "node ./tasks/update-post-timestamps.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}",
    "codegen": "graphql-codegen --config graphql-codegen.yml",
    "contentful:export": "contentful space export --config contentful.json --skip-content --skip-roles --skip-webhooks",
    "contentful:export:model": "contentful space export --config contentful.json",
    "contentful:import": "contentful space import --config contentful.json"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "@now/node": "^1.5.0",
    "@theme-ui/color": "^0.3.1",
    "@theme-ui/components": "^0.3.1",
    "@theme-ui/preset-deep": "^0.3.0",
    "@theme-ui/prism": "^0.3.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/react-query": "^1.1.2",
    "@types/ua-parser-js": "^0.7.33",
    "apollo-boost": "^0.4.7",
    "contentful-cli": "^0.36.2",
    "cross-env": "^7.0.2",
    "framer-motion": "^1.10.3",
    "gatsby": "^2.20.17",
    "gatsby-image": "^2.3.2",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.1.7",
    "gatsby-plugin-nprogress": "^2.2.1",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-check-links": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-numbered-footnotes": "^1.0.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-smartypants": "^2.2.1",
    "gatsby-remark-typescript": "^0.2.2",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.3",
    "gatsby-transformer-yaml": "^2.3.1",
    "gh-pages": "^2.2.0",
    "graphql": "^14.6.0",
    "instant.page": "^3.0.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "node-fetch": "^2.6.0",
    "prism-react-renderer": "^1.0.2",
    "query-string": "^6.12.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "react-medium-image-zoom": "^3.1.2",
    "react-outside-click-handler": "1.0.0",
    "react-query": "^1.2.1",
    "react-transition-group": "^4.3.0",
    "remark-slug": "^5.1.2",
    "semver": "^7.2.2",
    "theme-ui": "^0.3.1",
    "tiny-glob": "^0.2.6",
    "typeface-merriweather": "0.0.72",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/introspection": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/theme-ui": "^0.3.1",
    "concurrently": "^5.1.0",
    "delay-cli": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettify-ts-react": "^0.1.8",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "~1.19.1",
    "typescript": "~3.8.3"
  },
  "browserslist": [
    "chrome >= 57",
    "firefox >= 52",
    "edge >= 16",
    "safari >= 10.1",
    "opera >= 44",
    "ios >= 10.3",
    "ChromeAndroid >= 57"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{html,json,scss,css,md}": [
      "prettier --write"
    ],
    "content/**/*.{mdx,md}": [
      "node ./tasks/update-post-timestamps.js"
    ]
  },
  "engines": {
    "node": "12.x"
  }
}
