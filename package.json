{
  "name": "devrsi0n-blog",
  "private": true,
  "description": "Devrsi0n's personal blog",
  "version": "1.0.0",
  "author": "devrsi0n <devrsi0n@gmail.com>",
  "homepage": "https://devrsi0n.com",
  "keywords": [
    "devrsi0n",
    "next.js",
    "zeit",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "yarn run eslint",
    "update-last-updated-date": "node ./.tasks/update-post-timestamps.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cy:open",
    "test:e2e:ci": "BABEL_ENV=test start-server-and-test dev http://localhost:3000 cy:run",
    "eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "prettier": "prettier --write ./**/*.{json,html,css,scss,md,yml}"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/loader": "^1.6.5",
    "@mdx-js/mdx": "~1.6.1",
    "@mdx-js/react": "~1.6.1",
    "@now/node": "^1.6.1",
    "@theme-ui/color": "^0.3.1",
    "@theme-ui/components": "^0.3.1",
    "@theme-ui/preset-deep": "^0.3.0",
    "@theme-ui/prism": "^0.3.0",
    "@types/lodash": "^4.14.150",
    "@types/mdx-js__react": "^1.5.1",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-query": "^1.1.2",
    "@types/theme-ui": "^0.3.2",
    "@types/ua-parser-js": "^0.7.33",
    "apollo-boost": "^0.4.9",
    "babel-plugin-emotion": "^10.0.33",
    "contentful-cli": "^0.36.2",
    "cross-env": "^7.0.2",
    "date-fns": "^2.11.1",
    "file-loader": "^6.0.0",
    "framer-motion": "^1.11.0",
    "gh-pages": "^2.2.0",
    "globby": "^11.0.1",
    "graphql": "^15.1.0",
    "gray-matter": "^4.0.2",
    "instant.page": "^3.0.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.28",
    "next": "^9.4.4",
    "node-fetch": "^2.6.0",
    "prism-react-renderer": "^1.1.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-live": "^2.2.2",
    "react-medium-image-zoom": "^3.1.2",
    "react-outside-click-handler": "1.0.0",
    "react-query": "^1.5.3",
    "remark": "^12.0.0",
    "remark-code-extra": "^1.0.1",
    "remark-frontmatter": "^2.0.0",
    "remark-html": "^11.0.1",
    "remark-slug": "^6.0.0",
    "semver": "^7.3.2",
    "theme-ui": "^0.3.1",
    "tiny-glob": "^0.2.6",
    "typeface-merriweather": "0.0.72",
    "ua-parser-js": "^0.7.21",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@cypress/code-coverage": "^3.7.2",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "concurrently": "^5.2.0",
    "cypress": "^4.5.0",
    "delay-cli": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettify-ts-react": "^0.1.8",
    "husky": "^4.2.5",
    "istanbul-lib-coverage": "^3.0.0",
    "lint-staged": "^10.2.2",
    "nyc": "^15.0.1",
    "prettier": "~1.19.1",
    "start-server-and-test": "^1.11.0",
    "ts-loader": "^7.0.5",
    "typescript": "~3.9.2"
  },
  "browserslist": [
    "chrome >= 57",
    "firefox >= 52",
    "edge >= 16",
    "safari >= 10.1",
    "opera >= 44",
    "ios >= 10.3",
    "ChromeAndroid >= 57"
  ],
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "reporter": "html",
    "lines": 40,
    "include": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{html,json,scss,css,md}": [
      "prettier --write"
    ],
    "content/**/*.{mdx,md}": [
      "node ./.tasks/update-post-timestamps.js"
    ]
  },
  "engines": {
    "node": "12.x"
  },
  "browser": {
    "child_process": false
  }
}
